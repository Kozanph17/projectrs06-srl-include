Function P06_ItemExists(SlotToCheck: Integer): Boolean;
Var
  xsx,ysy,x,y,R,C:Integer;
Begin
  xsx:=569;
  ysy:=217;
  C:=(Round(SlotToCheck/4));
  R:=(SlotToCheck -(C*4));
  If FindColor(x, y, P06_outline_black, xsx+(R*42), ysy+(C*36), xsx+(R*42)+30, ysy+(C*36)+24) Then
  Begin
    Result:=True;
  End;
